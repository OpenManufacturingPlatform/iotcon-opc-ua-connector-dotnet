{
  "RootDirectory": null,
  "KeyVaultBaseUrl": null,
  "DisableSubscriptionRestoreService": true,
  "EnableMessageFilter": false,
  "ConnectorId": "###",
  "Logging": {
    "LogLevel": {
      "Default": "debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OpcUa": {
    "DefaultServerBrowseDepth": 3,
    "NodeBrowseDepth": 1,
    "EnableRegisteredNodes": false,
    "SubscriptionBatchSize": 100,
    "ReadBatchSize": 100,
    "RegisterNodeBatchSize": 100,
    "AwaitSessionLockTimeoutSecs": 3,
    "ReconnectIntervalSecs": 10,
    "NativeSettings": {
    }
  },
  "Communication": {
    "SchemaUrl": "https://someSchemaStore.com/schemas/",
    "Shared": {
      "Type": "mqtt",
      "NativeSettings": {
        "brokerAddress": "localhost",
        "brokerPort": "1883",
        "username": "test",
        "password": "test",
        "clientId": null,
        "secure": false,
        "caCertData": null,
        "clientCaCertData": null,
        "ignoreCertificateValidation": false,
        "cleanSession": true,
        "willFlag": false,
        "willQosLevel": 0,
        "willTopic": null,
        "willMessage": null,
        "willRetain": false,
        "keepAlivePeriod": 60,
        "autoReconnectTimeInSeconds": 10,
        "sslProtocols": "None",
        "protocolVersion": "Version_3_1"
      }
    },
    "TelemetryEndpoint": {
      "Type": "mqtt",
      "NativeSettings": {
        "topics": [
          {
            "topicName": "telemetry",
            "qosLevel": 1,
            "retain": false,
            "clientId": "TelemetryClientId"
          }
        ]
      }
    },
    "RequestEndpoint": {
      "Type": "mqtt",
      "NativeSettings": {
        "topics": [
          {
            "topicName": "commands",
            "qosLevel": 1,
            "retain": false,
            "clientId": "RequestsClientId"
          }
        ]
      }
    },
    "ResponseEndpoint": {
      "Type": "mqtt",
      "NativeSettings": {
        "topics": [
          {
            "topicName": "responses",
            "qosLevel": 1,
            "retain": false,
            "clientId": "ResponsesClientId"
          }
        ]
      }
    }
  },
  "Persistance": {
    "Type": "inMemory",
    "NativeSettings": {
    }
  }
}